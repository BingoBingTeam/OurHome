<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 底层白色 -->
    <item android:drawable="@color/status_bar_color_login" />

    <!-- 顶层Logo -->
    <item android:left="48dp" android:right="48dp" android:top="48dp" android:bottom="68dp"
        android:gravity="center_horizontal">
        <bitmap
            android:gravity="top"
            android:mipMap="true"
            android:antialias="true"
            android:dither="true"
            android:src="@mipmap/login_log_icon" />
    </item>
</layer-list>